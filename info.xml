<?xml version="1.0"?>
<extension key="nl.roparun.reports" type="module">
  <file>reports</file>
  <name>Roparun reports</name>
  <description>The following reports are included in this extension.
  	- Teams: Report with the team information and the teamcaptain. Used as export from CiviCRM to Roparun live.
	- TeamStanden: report with the total amount donated per team. Used for financial reporting.
	</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jaap Jansma</author>
    <email>jaap@edeveloper.nl</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/civicoop/nl.roparun.reports</url>
    <url desc="Documentation">https://github.com/civicoop/nl.roparun.reports/README.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-11-28</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Reports</namespace>
  </civix>
</extension>
